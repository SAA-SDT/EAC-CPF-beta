    <ref name="a.date"/>
    <ref name="a.type"/>
  </define>
  <!-- Alternative Date -->
  <define name="altdate">
    <element name="altdate">
      <ref name="attlist.altdate"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.altdate" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Place -->
  <define name="place">
    <element name="place">
      <ref name="attlist.place"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="part"/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.place" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <optional>
      <attribute name="placetype">
        <choice>
          <value>geog</value>
          <value>juris</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="normal"/>
    </optional>
    <ref name="a.type"/>
    <ref name="a.value"/>
  </define>
  <!-- Personal Name -->
  <define name="persname">
    <element name="persname">
      <ref name="attlist.persname"/>
      <ref name="m.name"/>
    </element>
  </define>
  <define name="attlist.persname" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Corporate Name -->
  <define name="corpname">
    <element name="corpname">
      <ref name="attlist.corpname"/>
      <ref name="m.name"/>
    </element>
  </define>
  <define name="attlist.corpname" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Family Name -->
  <define name="famname">
    <element name="famname">
      <ref name="attlist.famname"/>
      <ref name="m.name"/>
    </element>
  </define>
  <define name="attlist.famname" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Name Addition -->
  <define name="nameadd">
    <element name="nameadd">
      <ref name="attlist.nameadd"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.nameadd" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
  </define>
  <!-- Subject Term -->
  <define name="subject">
    <element name="subject">
      <ref name="attlist.subject"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="ignore"/>
          <ref name="part"/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.subject" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
    <ref name="a.value"/>
  </define>
  <!-- Genre/Form Term -->
  <define name="genreform">
    <element name="genreform">
      <ref name="attlist.genreform"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="ignore"/>
          <ref name="part"/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.genreform" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
    <ref name="a.value"/>
  </define>
  <!-- Part of Name -->
  <define name="part">
    <element name="part">
      <ref name="attlist.part"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
          <ref name="ignore"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.part" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
  </define>
  <!--
    Ignore Characters
    
     Used to designate characters used in headings or other entries
     that are to be ignored in sorting, indexing, display, or
     combinations.
    
     @type values:
       i   = index
       s   = sort
       d   = display
       di  = display-indexing
       is  = indexing-display
       ds  = display-sort
       all = display-index-sort
  -->
  <define name="ignore">
    <element name="ignore">
      <ref name="attlist.ignore"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.ignore" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <optional>
      <attribute name="type">
        <choice>
          <value>i</value>
          <value>s</value>
          <value>d</value>
          <value>di</value>
          <value>is</value>
          <value>ds</value>
          <value>all</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="authorized"/>
    </optional>
  </define>
  <!-- Address -->
  <define name="address">
    <element name="address">
      <ref name="attlist.address"/>
      <oneOrMore>
        <ref name="addressline"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.address" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Addressline -->
  <define name="addressline">
    <element name="addressline">
      <ref name="attlist.addressline"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base.ptrs"/>
          <ref name="extref"/>
          <ref name="ref"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.addressline" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <optional>
      <attribute name="type"/>
    </optional>
  </define>
  <!-- Descriptive Notes -->
  <define name="descnotes">
    <element name="descnotes">
      <ref name="attlist.descnotes"/>
      <optional>
        <ref name="head"/>
      </optional>
      <ref name="descnote"/>
      <oneOrMore>
        <ref name="descnote"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.descnotes" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Descriptive Note -->
  <define name="descnote">
    <element name="descnote">
      <ref name="attlist.descnote"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.para.content"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.descnote" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
  </define>
  <!-- Note -->
  <define name="note">
    <element name="note">
      <ref name="attlist.note"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.para.content"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.note" combine="interleave">
    <ref name="a.common"/>
    <optional>
      <attribute name="type"/>
    </optional>
  </define>
  <!-- Function/Activity Term -->
  <define name="funact">
    <element name="funact">
      <ref name="attlist.funact"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="ignore"/>
          <ref name="part"/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.funact" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
    <ref name="a.value"/>
  </define>
  <!-- ===================================================================== -->
  <!-- OASIS-OPEN XML Exchange Table Model Entity -->
  <!-- ===================================================================== -->
  <!--
    Binary
    
    Used in Oasis xml table model: zero(s) = no; anything
    else = yes
  -->
  <define name="yesorno">
    <data type="NMTOKEN"/>
  </define>
  <!-- ======================================================================= -->
  <!-- ======================================================================= -->
  <!--
    Table Element Declarations
    
    Oasis XML Exchange Table Model
    
    Note: Modified by expanding parameter entities; adding a.common
    attributes to non-empty elements; title element is eac head
    element. entry content is defined in parameter entity section
    Parameter Entities for Elements above.
  -->
  <!-- Table -->
  <define name="table">
    <element name="table">
      <ref name="attlist.table"/>
      <optional>
        <ref name="head"/>
      </optional>
      <oneOrMore>
        <ref name="tgroup"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.table" combine="interleave">
    <ref name="a.common"/>
    <optional>
      <attribute name="frame">
        <choice>
          <value>top</value>
          <value>bottom</value>
          <value>topbot</value>
          <value>all</value>
          <value>sides</value>
          <value>none</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="colsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="rowsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="pgwide">
        <ref name="yesorno"/>
      </attribute>
    </optional>
  </define>
  <!-- Table Group -->
  <define name="tgroup">
    <element name="tgroup">
      <ref name="attlist.tgroup"/>
      <zeroOrMore>
        <ref name="colspec"/>
      </zeroOrMore>
      <optional>
        <ref name="thead"/>
      </optional>
      <ref name="tbody"/>
    </element>
  </define>
  <define name="attlist.tgroup" combine="interleave">
    <ref name="a.common"/>
    <attribute name="cols">
      <data type="NMTOKEN"/>
    </attribute>
    <optional>
      <attribute name="colsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="rowsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="align">
        <choice>
          <value>left</value>
          <value>right</value>
          <value>center</value>
          <value>justify</value>
          <value>char</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- Column Specification -->
  <define name="colspec">
    <element name="colspec">
      <ref name="attlist.colspec"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.colspec" combine="interleave">
    <optional>
      <attribute name="colnum">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="colname">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="colwidth"/>
    </optional>
    <optional>
      <attribute name="colsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="rowsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="align">
        <choice>
          <value>left</value>
          <value>right</value>
          <value>center</value>
          <value>justify</value>
          <value>char</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="char"/>
    </optional>
    <optional>
      <attribute name="charoff">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!-- Table Head -->
  <define name="thead">
    <element name="thead">
      <ref name="attlist.thead"/>
      <oneOrMore>
        <ref name="row"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.thead" combine="interleave">
    <ref name="a.common"/>
    <optional>
      <attribute name="valign">
        <choice>
          <value>top</value>
          <value>middle</value>
          <value>bottom</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- Table Body -->
  <define name="tbody">
    <element name="tbody">
      <ref name="attlist.tbody"/>
      <oneOrMore>
        <ref name="row"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.tbody" combine="interleave">
    <ref name="a.common"/>
    <optional>
      <attribute name="valign">
        <choice>
          <value>top</value>
          <value>middle</value>
          <value>bottom</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- Row -->
  <define name="row">
    <element name="row">
      <ref name="attlist.row"/>
      <oneOrMore>
        <ref name="entry"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.row" combine="interleave">
    <ref name="a.common"/>
    <optional>
      <attribute name="rowsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="valign">
        <choice>
          <value>top</value>
          <value>middle</value>
          <value>bottom</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- Entry -->
  <define name="entry">
    <element name="entry">
      <ref name="attlist.entry"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.table.entry"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.entry" combine="interleave">
    <ref name="a.common"/>
    <optional>
      <attribute name="colname">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="namest">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="nameend">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="morerows">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="colsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="rowsep">
        <ref name="yesorno"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="align">
        <choice>
          <value>left</value>
          <value>right</value>
          <value>center</value>
          <value>justify</value>
          <value>char</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="char"/>
    </optional>
    <optional>
      <attribute name="charoff">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="valign">
        <choice>
          <value>top</value>
          <value>middle</value>
          <value>bottom</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <start>
    <choice>
      <ref name="eac"/>
    </choice>
  </start>
</grammar>
