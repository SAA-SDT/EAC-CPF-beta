<?xml version="1.0" encoding="UTF-8"?>
<!--
  20040809   
-->

<!-- ======================================================================= -->
<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" ns="http://jefferson.village.virginia.edu/eac" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <define name="m.ptrs">
    <choice>
      <ref name="ptr"/>
      <ref name="extptr"/>
    </choice>
  </define>
  <define name="m.refs">
    <choice>
      <ref name="ref"/>
      <ref name="extref"/>
      <ref name="bibref"/>
      <ref name="title"/>
      <ref name="archref"/>
      <ref name="note"/>
    </choice>
  </define>
  <define name="m.access">
    <choice>
      <ref name="corpname"/>
      <ref name="famname"/>
      <ref name="persname"/>
      <ref name="funact"/>
      <ref name="subject"/>
      <ref name="genreform"/>
    </choice>
  </define>
  <define name="m.data">
    <choice>
      <ref name="m.access"/>
      <ref name="date"/>
      <ref name="num"/>
      <ref name="place"/>
    </choice>
  </define>
  <define name="m.base">
    <choice>
      <ref name="emph"/>
      <ref name="lb"/>
      <ref name="abbr"/>
      <ref name="expan"/>
    </choice>
  </define>
  <define name="m.base.ptrs">
    <choice>
      <ref name="m.base"/>
      <ref name="m.ptrs"/>
    </choice>
  </define>
  <define name="m.base.ptrs.refs">
    <choice>
      <ref name="m.base.ptrs"/>
      <ref name="m.refs"/>
    </choice>
  </define>
  <define name="m.base.ptrs.data">
    <choice>
      <ref name="m.base.ptrs"/>
      <ref name="m.data"/>
    </choice>
  </define>
  <define name="m.base.ptrs.refs.data">
    <choice>
      <ref name="m.base.ptrs"/>
      <ref name="m.data"/>
      <ref name="m.refs"/>
    </choice>
  </define>
  <define name="m.inter">
    <choice>
      <ref name="address"/>
      <ref name="chronlist"/>
      <ref name="list"/>
      <ref name="table"/>
    </choice>
  </define>
  <define name="m.inter.quote">
    <choice>
      <ref name="m.inter"/>
      <ref name="blockquote"/>
    </choice>
  </define>
  <define name="m.blocks">
    <choice>
      <ref name="m.inter.quote"/>
      <ref name="p"/>
    </choice>
  </define>
  <!-- note: m.block.noquote used in content model of blockquote -->
  <define name="m.blocks.noquote">
    <choice>
      <ref name="m.inter"/>
      <ref name="p"/>
    </choice>
  </define>
  <define name="m.para.content">
    <choice>
      <ref name="m.base.ptrs.refs.data"/>
      <ref name="m.inter.quote"/>
    </choice>
  </define>
  <define name="m.para.content.norefs">
    <choice>
      <ref name="m.base.ptrs.data"/>
      <ref name="m.inter"/>
    </choice>
  </define>
  <define name="m.table.entry">
    <choice>
      <ref name="m.base.ptrs.refs.data"/>
      <ref name="address"/>
      <ref name="list"/>
    </choice>
  </define>
  <define name="m.arch">
    <choice>
      <ref name="abstract"/>
      <ref name="physdesc"/>
      <ref name="descnote"/>
      <ref name="repository"/>
      <ref name="origination"/>
      <ref name="unitdate"/>
      <ref name="unitid"/>
      <ref name="unittitle"/>
      <ref name="container"/>
      <ref name="physloc"/>
      <ref name="langmaterial"/>
      <ref name="materialspec"/>
    </choice>
  </define>
  <define name="m.namehead">
    <oneOrMore>
      <ref name="part"/>
    </oneOrMore>
    <group>
      <zeroOrMore>
        <choice>
          <ref name="existdate"/>
          <ref name="place"/>
          <ref name="nameadd"/>
        </choice>
      </zeroOrMore>
      <optional>
        <ref name="usedate"/>
      </optional>
      <optional>
        <choice>
          <ref name="sourceref"/>
          <ref name="sourcerefs"/>
        </choice>
      </optional>
      <optional>
        <choice>
          <ref name="descnote"/>
          <ref name="descnotes"/>
        </choice>
      </optional>
    </group>
  </define>
  <define name="m.bibl">
    <choice>
      <text/>
      <ref name="name"/>
      <ref name="title"/>
      <ref name="edition"/>
      <ref name="imprint"/>
      <ref name="bibseries"/>
      <ref name="descnote"/>
    </choice>
  </define>
  <define name="m.name">
    <zeroOrMore>
      <choice>
        <text/>
        <ref name="m.base"/>
      </choice>
    </zeroOrMore>
  </define>
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- A. Parameter Entities for Attributes -->
  <!-- ======================================================================= -->
  <define name="id.type">
    <data type="ID"/>
  </define>
  <define name="idref.type">
    <data type="IDREF"/>
  </define>
  <define name="a.common">
    <optional>
      <attribute name="id">
        <ref name="id.type"/>
      </attribute>
    </optional>
  </define>
  <define name="a.ea">
    <optional>
      <attribute name="ea">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    a.sys, a.syskey, a.type, and a.value: consitute different
    classes used in controlled type, values, rules, and
    sources
  -->
  <define name="a.sys">
    <optional>
      <attribute name="system">
        <data type="ENTITY"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="countrycode">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="ownercode">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <define name="a.syskey">
    <optional>
      <attribute name="syskey"/>
    </optional>
    <optional>
      <attribute name="system">
        <data type="ENTITY"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="countrycode">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="ownercode">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <define name="a.type">
    <optional>
      <attribute name="type"/>
    </optional>
    <optional>
      <attribute name="typeauth">
        <ref name="idref.type"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="typekey"/>
    </optional>
  </define>
  <define name="a.value">
    <optional>
      <attribute name="valueauth">
        <ref name="idref.type"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="valuekey"/>
    </optional>
  </define>
  <define name="a.lang">
    <optional>
      <attribute name="languagecode">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="scriptcode">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <define name="a.head">
    <optional>
      <attribute name="rule">
        <ref name="idref.type"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="authorized"/>
    </optional>
  </define>
  <define name="a.date">
    <optional>
      <attribute name="scope">
        <choice>
          <value>begin</value>
          <value>end</value>
          <value>active</value>
          <value>begin-end</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="form">
        <choice>
          <value>single</value>
          <value>closedspan</value>
          <value>openspan</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="era"/>
    </optional>
    <optional>
      <attribute name="calendar"/>
    </optional>
    <optional>
      <attribute name="normal"/>
    </optional>
  </define>
  <define name="av.render">
    <choice>
      <value>bold</value>
      <value>italic</value>
      <value>bolditalic</value>
      <value>boldunderline</value>
      <value>boldsmcaps</value>
      <value>boldquoted</value>
      <value>nonproport</value>
      <value>altrender</value>
      <value>quoted</value>
      <value>smcaps</value>
      <value>sub</value>
      <value>super</value>
      <value>underline</value>
    </choice>
  </define>
  <!--
    linking attributes. this model employs xlink attributes,
    but does not employ the xlink namespace because it is
    is problematic. xslt can be used to convert instance
    using this dtd into instance with xlink namespace
    declarations and prefixing of appropriate attributes.
  -->
  <define name="a.simple">
    <optional>
      <attribute name="linktype" a:defaultValue="simple">
        <value>simple</value>
      </attribute>
    </optional>
    <optional>
      <attribute name="href"/>
    </optional>
    <optional>
      <attribute name="role"/>
    </optional>
    <optional>
      <attribute name="arcrole"/>
    </optional>
    <optional>
      <attribute name="title"/>
    </optional>
    <optional>
      <attribute name="show">
        <choice>
          <value>new</value>
          <value>replace</value>
          <value>embed</value>
          <value>showother</value>
          <value>shownone</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="actuate">
        <choice>
          <value>onload</value>
          <value>onrequest</value>
          <value>actuateother</value>
          <value>actuatenone</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <define name="a.simple.in">
    <optional>
      <attribute name="targetin">
        <ref name="idref.type"/>
      </attribute>
    </optional>
    <ref name="a.simple"/>
  </define>
  <define name="a.simple.out">
    <optional>
      <attribute name="targetout">
        <data type="ENTITY"/>
      </attribute>
    </optional>
    <ref name="a.simple"/>
  </define>
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!--
    PART III
    ENCODED ARCHIVAL CONTEXT ELEMENT AND ATTRIBUTE DECLARATIONS
  -->
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- ======================================================================= -->
  <!-- ======================================================================= -->
  <!-- Encoded Archival Context Group -->
  <!-- ======================================================================= -->
  <!-- Encoded Archival Context Group -->
  <define name="eac">
    <element name="eac">
      <ref name="attlist.eac"/>
      <ref name="eacheader"/>
      <ref name="condesc"/>
    </element>
  </define>
  <define name="attlist.eac" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <attribute name="type">
      <choice>
        <value>corpname</value>
        <value>persname</value>
        <value>famname</value>
      </choice>
    </attribute>
  </define>
  <!-- ======================================================================= -->
  <!-- Encoded Archival Context Header -->
  <define name="eacheader">
    <element name="eacheader">
      <ref name="attlist.eacheader"/>
      <ref name="eacid"/>
      <ref name="mainhist"/>
      <optional>
        <ref name="languagedecl"/>
      </optional>
      <optional>
        <ref name="ruledecl"/>
      </optional>
      <optional>
        <ref name="sourcedecl"/>
      </optional>
      <optional>
        <ref name="authdecl"/>
      </optional>
    </element>
  </define>
  <define name="attlist.eacheader" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <attribute name="status">
      <choice>
        <value>draft</value>
        <value>edited</value>
        <value>deleted</value>
      </choice>
    </attribute>
    <optional>
      <attribute name="langencoding" a:defaultValue="iso639-2b">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="scriptencoding" a:defaultValue="iso15924">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="dateencoding" a:defaultValue="iso8601">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="countryencoding" a:defaultValue="iso3166-1">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="ownerencoding" a:defaultValue="iso15511">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="encodinganalogsys">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="detaillevel">
        <choice>
          <value>minimal</value>
          <value>partial</value>
          <value>full</value>
        </choice>
      </attribute>
    </optional>
    <ref name="a.type"/>
  </define>
  <!-- ======================================================================= -->
  <!-- Encoded Archival Context Identifier -->
  <define name="eacid">
    <element name="eacid">
      <ref name="attlist.eacid"/>
      <text/>
    </element>
  </define>
  <define name="attlist.eacid" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.syskey"/>
  </define>
  <!-- ======================================================================= -->
  <!-- Context Description -->
  <define name="condesc">
    <element name="condesc">
      <ref name="attlist.condesc"/>
      <optional>
        <ref name="head"/>
      </optional>
      <group>
        <ref name="identity"/>
        <optional>
          <ref name="desc"/>
        </optional>
        <optional>
          <ref name="eacrels"/>
        </optional>
        <optional>
          <ref name="resourcerels"/>
        </optional>
        <optional>
          <ref name="funactrels"/>
        </optional>
      </group>
    </element>
  </define>
  <define name="attlist.condesc" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- ======================================================================= -->
  <!-- Context Description Group -->
  <!-- ======================================================================= -->
  <!-- Identity -->
  <define name="identity">
    <element name="identity">
      <ref name="attlist.identity"/>
      <optional>
        <ref name="head"/>
      </optional>
      <group>
        <zeroOrMore>
          <ref name="legalid"/>
        </zeroOrMore>
        <choice>
          <oneOrMore>
            <choice>
              <ref name="persgrp"/>
              <ref name="pershead"/>
            </choice>
          </oneOrMore>
          <oneOrMore>
            <choice>
              <ref name="corpgrp"/>
              <ref name="corphead"/>
            </choice>
          </oneOrMore>
          <oneOrMore>
            <choice>
              <ref name="famgrp"/>
              <ref name="famhead"/>
            </choice>
          </oneOrMore>
        </choice>
        <zeroOrMore>
          <ref name="nameadds"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="didentifier"/>
        </zeroOrMore>
        <group>
          <optional>
            <choice>
              <ref name="sourceref"/>
              <ref name="sourcerefs"/>
            </choice>
          </optional>
          <optional>
            <choice>
              <ref name="descnote"/>
              <ref name="descnotes"/>
            </choice>
          </optional>
        </group>
      </group>
    </element>
  </define>
  <define name="attlist.identity" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Legal Identifier -->
  <define name="legalid">
    <element name="legalid">
      <ref name="attlist.legalid"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="part"/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.legalid" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.type"/>
    <ref name="a.value"/>
  </define>
  <!-- Personal Name Heading Group -->
  <define name="persgrp">
    <element name="persgrp">
      <ref name="attlist.persgrp"/>
      <group>
        <ref name="pershead"/>
        <oneOrMore>
          <ref name="pershead"/>
        </oneOrMore>
      </group>
      <zeroOrMore>
        <ref name="nameadds"/>
      </zeroOrMore>
      <optional>
        <choice>
          <ref name="sourceref"/>
          <ref name="sourcerefs"/>
        </choice>
      </optional>
      <optional>
        <choice>
          <ref name="descnote"/>
          <ref name="descnotes"/>
        </choice>
      </optional>
    </element>
  </define>
  <define name="attlist.persgrp" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Personal Name Heading -->
  <define name="pershead">
    <element name="pershead">
      <ref name="attlist.pershead"/>
      <ref name="m.namehead"/>
    </element>
  </define>
  <define name="attlist.pershead" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.head"/>
    <ref name="a.lang"/>
    <ref name="a.type"/>
  </define>
  <!-- Corporate Body Heading Group -->
  <define name="corpgrp">
    <element name="corpgrp">
      <ref name="attlist.corpgrp"/>
      <group>
        <ref name="corphead"/>
        <oneOrMore>
          <ref name="corphead"/>
        </oneOrMore>
      </group>
      <zeroOrMore>
        <ref name="nameadds"/>
      </zeroOrMore>
      <optional>
        <choice>
          <ref name="sourceref"/>
          <ref name="sourcerefs"/>
        </choice>
      </optional>
      <optional>
        <choice>
          <ref name="descnote"/>
          <ref name="descnotes"/>
        </choice>
      </optional>
    </element>
  </define>
  <define name="attlist.corpgrp" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Corporate Body Heading -->
  <define name="corphead">
    <element name="corphead">
      <ref name="attlist.corphead"/>
      <ref name="m.namehead"/>
    </element>
  </define>
  <define name="attlist.corphead" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.head"/>
    <ref name="a.lang"/>
    <ref name="a.type"/>
  </define>
  <!-- Family Heading Group -->
  <define name="famgrp">
    <element name="famgrp">
      <ref name="attlist.famgrp"/>
      <group>
        <ref name="famhead"/>
        <oneOrMore>
          <ref name="famhead"/>
        </oneOrMore>
      </group>
      <zeroOrMore>
        <ref name="nameadds"/>
      </zeroOrMore>
      <optional>
        <choice>
          <ref name="sourceref"/>
          <ref name="sourcerefs"/>
        </choice>
      </optional>
      <optional>
        <choice>
          <ref name="descnote"/>
          <ref name="descnotes"/>
        </choice>
      </optional>
    </element>
  </define>
  <define name="attlist.famgrp" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Family Heading -->
  <define name="famhead">
    <element name="famhead">
      <ref name="attlist.famhead"/>
      <ref name="m.namehead"/>
    </element>
  </define>
  <define name="attlist.famhead" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.head"/>
    <ref name="a.lang"/>
    <ref name="a.type"/>
  </define>
  <!-- Digital Identifier -->
  <define name="didentifier">
    <element name="didentifier">
      <ref name="attlist.didentifier"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.didentifier" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.simple.out"/>
  </define>
  <!-- Name Additions -->
  <define name="nameadds">
    <element name="nameadds">
      <ref name="attlist.nameadds"/>
      <zeroOrMore>
        <choice>
          <ref name="place"/>
          <ref name="existdate"/>
          <ref name="nameadd"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.nameadds" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.lang"/>
  </define>
  <!-- Existence Date -->
  <define name="existdate">
    <element name="existdate">
      <ref name="attlist.existdate"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
          <ref name="altdate"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.existdate" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.date"/>
    <ref name="a.type"/>
  </define>
  <!-- Use Date -->
  <define name="usedate">
    <element name="usedate">
      <ref name="attlist.usedate"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
          <ref name="altdate"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.usedate" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <ref name="a.date"/>
    <ref name="a.type"/>
  </define>
  <!-- Source References -->
  <define name="sourcerefs">
    <element name="sourcerefs">
      <ref name="attlist.sourcerefs"/>
      <optional>
        <ref name="head"/>
      </optional>
      <ref name="sourceref"/>
      <oneOrMore>
        <ref name="sourceref"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.sourcerefs" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- Source Reference -->
  <define name="sourceref">
    <element name="sourceref">
      <ref name="attlist.sourceref"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="m.base"/>
          <ref name="sourceinfo"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.sourceref" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
    <optional>
      <attribute name="sourceref">
        <ref name="idref.type"/>
      </attribute>
    </optional>
  </define>
  <!-- EAC Relations -->
  <define name="eacrels">
    <element name="eacrels">
      <ref name="attlist.eacrels"/>
      <optional>
        <ref name="head"/>
      </optional>
      <oneOrMore>
        <ref name="eacrel"/>
      </oneOrMore>
      <optional>
        <choice>
          <ref name="sourceref"/>
          <ref name="sourcerefs"/>
        </choice>
      </optional>
      <optional>
        <choice>
          <ref name="descnote"/>
          <ref name="descnotes"/>
        </choice>
      </optional>
    </element>
  </define>
  <define name="attlist.eacrels" combine="interleave">
    <ref name="a.common"/>
    <ref name="a.ea"/>
  </define>
  <!-- EAC Relation -->
  <define name="eacrel">
    <element name="eacrel">
      <ref name="attlist.eacrel"/>
      <choice>
        <ref name="persname"/>
        <ref name="corpname"/>
        <ref name="famname"/>
      </choice>
      <group>
        <optional>
          <ref name="date"/>
